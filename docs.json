{
  "$schema": "https://mintlify.com/docs.json",
  "name": "Qwen CLI",
  "theme": "mint",
  "logo": {
    "light": "/logo/light-logo.png",
    "dark": "/logo/dark-logo.png"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#2563EB",
    "light": "#3B82F6",
    "dark": "#1D4ED8"
  },
  "redirects": [
    {
      "source": "/",
      "destination": "/landing"
    }
  ],
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "landing",
              "introduction",
              "quickstart", 
              "authentication"
            ]
          },
          {
            "group": "Model Integrations",
            "pages": [
              "integrations/qwen3-coder",
              "integrations/kimi-k2",
              "integrations/grok-4"
            ]
          },
          {
            "group": "Examples",
            "pages": [
              "examples/basic-usage"
            ]
          },
          {
            "group": "API Reference",
            "pages": [
              "api-reference/commands"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Get API Key",
          "href": "https://openrouter.ai",
          "icon": "key"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/qwen-code/qwen-code",
          "icon": "github"
        },
        {
          "anchor": "Community",
          "href": "https://discord.gg/qwen",
          "icon": "discord"
        }
      ]
    }
  },
  "footer": {
    "socials": {
      "twitter": "https://twitter.com/qwencode",
      "github": "https://github.com/qwen-code/qwen-code",
      "discord": "https://discord.gg/qwen"
    }
  }
}